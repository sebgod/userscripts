[user]
	name = "Sebastian Godelet"
	email = sebastian.godelet+github@gmail.com
[credential]
[core]
	autocrlf = false
	symlinks = true
[alias]
	gr = log --graph --full-history --all --color --abbrev-commit --pretty=tformat:"%x1b[31m%h%x09%x1b[32m%d%x1b[0m%x20%s%x20%x1b[32m%x20(%cr)%x1b[33m%x20<%an>%x1b[0m"
	snu = status --untracked=no
	up = !git remote update -p; git merge --ff-only @{u}
	bv = branch -vv
	resetorigin = !git fetch origin && git reset --hard origin/master && git clean -f -d
	resetupstream = !git fetch upstream && git reset --hard upstream/master && git clean -f -d
[push]
	default = current

[credential]
	helper = store
[color]
	ui = true
[sendemail]
	envelopesender = sebastian.godelet+github@gmail.com
	from = sebastian.godelet+github@gmail.com
